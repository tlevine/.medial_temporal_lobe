<script type="text/javascript">
    var ddlTipoProduto = "#ctl00_UCFilial1__ddlTipoProduto";
    var ddlTipo = "#ctl00_UCFilial1__ddlTipo";
    var txtCep1 = "#ctl00_UCFilial1__txtCep1";
    var txtCep2 = "#ctl00_UCFilial1__txtCep2";

    function TipoProdutoChange()
    {
        if ($(ddlTipoProduto).val() == 3)
        {
            $(ddlTipo).val('REV');
            $("#_divTipo").css({ "display": "block" });
        }
        else
        {
            $("#_divTipo").css({ "display": "none" });
            $('#_divCep').css({ "display": "block" });
        }
        TipoChange();
    }

    function TipoChange()
    {
        if ($(ddlTipo).val() == 'REP')
        {
            $('#_divCep').css({ "display": "none" });
            $(txtCep1).val("");
            $(txtCep2).val("");
        }
        else
        {
            $('#_divCep').css({ "display": "block" });
        }
        LoadEstados();
    }

    function LoadEstados()
    {
        $.getJSON(
        '/WebControls/Filiais/GetEstados.ashx?pag_id=' + $(ddlTipoProduto).val() + '&tipo=' + $(ddlTipo).val(),
        function (dados)
        {
            var options = "";
            $.each(dados, function (key, value)
            {
                options += '<option value="' + value.text + '">' + value.value + '</option>';
            });
            $("#_ddlEstado").html(options);
            var newLabel = $("#_ddlEstado").children('option:selected').text();
            var $fakeSelect = $("#_ddlEstado").parent('.fake_select');
            $fakeSelect.children('.label').text(newLabel);
            LoadCidade();
        });
    };

    function LoadCidade()
    {
        $.getJSON(
        '/WebControls/Filiais/GetCidades.ashx?pag_id=' + $(ddlTipoProduto).val() + '&uf=' + $("#_ddlEstado").val() + '&tipo=' + $(ddlTipo).val(),
        function (dados)
        {
            var options = "";
            $.each(dados, function (key, value)
            {
                options += '<option value="' + value.text + '">' + value.value + '</option>';
            });
            $("#_ddlCidade").html(options);
            var newLabel = $("#_ddlCidade").children('option:selected').text();
            var $fakeSelect = $("#_ddlCidade").parent('.fake_select');
            $fakeSelect.children('.label').text(newLabel);
        });
    };


    $(function ()
    {
        $(ddlTipoProduto).change(TipoProdutoChange);
        $(ddlTipo).change(TipoChange);
        $("#_ddlEstado").change(LoadCidade);
    });
</script>

            

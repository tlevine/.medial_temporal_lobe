<pre> 
cat &lt;file> |tr -s '=' ' '|sort -k5n | awk '{NFIVE=NR*.95}; {if (NR&lt;94222) TOTAL+=$5} END{printf("COUNT:%d, TOTAL:%d,MEAN:%d\n",NFIVE,TOTAL,TOTAL/NFIVE)}'

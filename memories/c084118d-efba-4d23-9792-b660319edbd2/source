<script type="text/javascript">
  $('body header:first-of-type nav ul li a').click(function () {
    var el = $(this).attr("href"),
        headerHeight = $('body header:first-of-type').height(),
        dest = $(el).offset().top-headerHeight;
    $(this).parent().siblings().removeClass('current');
    $(this).parent().addClass('current');
    $("html:not(:animated),body:not(:animated)").animate({ scrollTop: dest-20}, 500 );
    return false;
  });
</script>


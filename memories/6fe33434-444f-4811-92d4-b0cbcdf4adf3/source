if (typeof aa !== 'undefined' && typeof aa.kvscreenwidth !== 'undefined')
  aa.target += 'kvscreenwidth=' + escape(aa.kvscreenwidth()) + ';';
aa.src = aa.adServer + 'addyn' + aa.target + 'grp=' + aa.pageNum +';misc=' + aa.random();
document.write("<scr");
document.write("ipt src='" + aa.src + "'>");
document.write("</scr");
document.write("ipt>");
</script>

<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        var narrowGb = $('#Obituary .GuestbookNarrow');
        var numComments = narrowGb.find('.GuestBookEntry ul li').length;
        if (numComments > 0) {
            narrowGb.find('.GuestBookEntry ul li:first').show();
            if (numComments > 1)
                setTimeout("GBNarrowRotate()", 8000);
        }
    });
    function GBNarrowRotate() {
        var narrowGb = $('#Obituary .GuestbookNarrow');
        narrowGb.find('.GuestBookEntry ul li:first').hide();
        narrowGb.find('.GuestBookEntry ul li:last').after(narrowGb.find('.GuestBookEntry ul li:first'));
        narrowGb.find('.GuestBookEntry ul li:first').show();
        setTimeout("GBNarrowRotate()", 8000);
    }
</script>

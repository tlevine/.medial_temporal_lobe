    <script type="text/javascript">
        $(document).ready(function () {
            var ad = $('#ObitBottomAd a:first');
            if (ad.length > 0) {
                var href = ad.attr('href');
                if (href != null && href.toLowerCase().indexOf('adid=-') > 0) { // ad impression is empty
                    $('#ObitGuestBookTwoColumn').show();
                    $('#ObitGuestBookOneColumn .GuestBookAddEntry').attr('name', '');
                }
                else {
                    $('#ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_ObitGuestBookWithAd').show();
                    $('#ObitGuestBookTwoColumn .GuestBookAddEntry').attr('name', '');
                }
            }
            else {
                $('#ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_ObitGuestBookWithAd').show();
                $('#ObitGuestBookTwoColumn .GuestBookAddEntry').attr('name', '');
            }
            var ad = $('#ObitBottomWideAd a:first');
            if (ad.length > 0) {
                var href = ad.attr('href');
                if (href != null && href.toLowerCase().indexOf('adid=-') > 0) { // ad impression is empty hide it
                    $('#ObitBottomWideAd').hide();
                }
            }
        });
    </script>


<div id="ctl00_ContentPlaceHolder_updPnlCartSnapshot">
	
    


<script type="text/javascript">
    var ys = new Array();
    var xs = new Array();

    $(document).ready(function () {
        $(document).mousemove(function (e) {
            ys = [];
            xs = [];
            ys.push(e.pageY);
            xs.push(e.pageX);
        });

        $('#ctl00_ContentPlaceHolder_CartItemSnapshot_ItemCount').val($("a[id^=cart.]").size());
    })

    var timerRunning = false;
    var myTimer = null;
    var CartSnapshotPrefChecked = false;

    function ShowItemPopup(time) {
        if ($('#ctl00_ContentPlaceHolder_CartItemSnapshot_ShowCartSnapshot').val() == "True") {
            $find('popupBehaviorItem')._offsetX = Math.round(($(window).width() - $("#ctl00_ContentPlaceHolder_CartItemSnapshot_pnlItem").width()) / 2);
            $find('popupBehaviorItem')._offsetY = Math.round(($(window).height() - $("#ctl00_ContentPlaceHolder_CartItemSnapshot_pnlItem").height()) / 2);
            $find('popupBehaviorItem').showPopup();

            if (timerRunning) {
                clearTimeout(myTimer);
                timerRunning = false;
            }
            myTimer = setTimeout("ValidateCloseItemPopup();", time);
            timerRunning = true;
        }
    }

    function ValidateCloseItemPopup() {
        var pnlTopOffset = $("#ctl00_ContentPlaceHolder_CartItemSnapshot_pnlItem").offset().top;
        var pnlLeftOffset = $("#ctl00_ContentPlaceHolder_CartItemSnapshot_pnlItem").offset().left;
        var pnlWidth = $("#ctl00_ContentPlaceHolder_CartItemSnapshot_pnlItem").width();
        var pnlHeight = $("#ctl00_ContentPlaceHolder_CartItemSnapshot_pnlItem").height()

        // Root problem the e.PageY was showing incorrect value when using reg variable and hovering over the description hyperlink.
        lastY = ys[ys.length - 1];
        lastX = xs[xs.length - 1];
        if (lastX >= pnlLeftOffset && lastX <= pnlLeftOffset + pnlWidth && lastY >= pnlTopOffset && lastY <= pnlTopOffset + pnlHeight) {
            myTimer = setTimeout("ValidateCloseItemPopup();", 100);
            timerRunning = true;
        }
        else {

            CloseItemPopup();
        }
    }

    function CloseItemPopup() {
        $('#ctl00_ContentPlaceHolder_CartItemSnapshot_ShowCartSnapshot').val("False");
        clearTimeout(myTimer);
        timerRunning = false;
        $find('popupBehaviorItem').hidePopup();
        if (CartSnapshotPrefChecked == true) {
            CartSnapshotPrefChecked = false;
            if ($('#ctl00_ContentPlaceHolder_CartItemSnapshot_chkCartSnapshotPref').length > 0)
                { javascript: __doPostBack('ctl00_ContentPlaceHolder_CartItemSnapshot_chkCartSnapshotPref', ''); }
            }
    }

    function CartSnapshotPrefChanged() {
        clearTimeout(myTimer);
        timerRunning = false;
        //if checked then make it true.. otherwise false
        if($('#ctl00_ContentPlaceHolder_CartItemSnapshot_chkCartSnapshotPref').length>0 && $('#ctl00_ContentPlaceHolder_CartItemSnapshot_chkCartSnapshotPref').val()=="on")
        { CartSnapshotPrefChecked = true; }
    }

    function UpdateCartTotal(cartTotalId, total) {
        var cartTotal = $("#" + cartTotalId);
        if (cartTotal != null) {
            cartTotal.text(total);
        }
    }
</script>


	<script type="text/javascript">
        function ToggleText() {
        var isLargerText = (getCookieSubValue('UlineCom', 'IsLargeFont') == 'true'),
        hlfont = document.getElementById('ctl00_hlFontSize'),
        hlfontimage = document.getElementById('ctl00_hlFontSizeImage'),
        css = document.getElementById('ctl00_lnkCssFile');
        
        if (isLargerText) {
            css.href = "/FilesCombiner.ashx?content=css&version=12-19-2012";

            hlfontimage.innerHTML = "<div id='divfontimg' class='spritetop' style='background-position: -0px -248px; width: 14px; height: 13px'></div>";
            hlfont.innerHTML = "Larger Text";
            setCookieSubValue('UlineCom', 'IsLargeFont', 'false', 180, cookieDomain);
        }
        else {
            css.href = "/FilesCombiner.ashx?content=csslarge&version=12-19-2012";

            hlfontimage.innerHTML = "<div id='divfontimg' class='spritetop' style='background-position: -0px -271px; width: 14px; height: 13px'></div>";
            hlfont.innerHTML = "Regular Text";
            setCookieSubValue('UlineCom', 'IsLargeFont', 'true', 180, cookieDomain);
        }
    }    

        var autocompleteSelected = false;
        $(document).ready(function()
        {
            var txtProductSearch = $('#ctl00_txtProductSearch');
            if (true)
            {
                var data = "/Product/KeywordSearchService.asmx/GetCompletionListJquery";
                txtProductSearch
                .autocomplete(data,
                {
                    width: "300px",
                    minChars: "2",
                    max: "15",
                    matchSubset: false,
                    selectFirst: false,
                    scrollHeight: "9999", // Eliminate scroll bars
                    delay: 100,
                    parse: function(data)
                    {
                        var parsed = [];
                        $("SearchResult", data).each(function(i)
                        {
                            var row = $(this).find("Search").text();
                            parsed[parsed.length] = {
                                data: row.split(),
                                value: txtProductSearch.val(),
                                result: row
                            };
                        });
                        return parsed;
                    }
                })
                .result(function(event, data, formatted)
                {
                    autocompleteSelected = true;
                    AutoCompleteSearchLink('/Product/AdvSearchResult.aspx?KeywordTyped=' + escape(formatted) + '&keywords=', data[0], '', '/Product/SearchNotFound.aspx?keywords=');
                });
            }
            // Execute search if enter key is pressed in search text box.
            txtProductSearch.keydown(function(event)
            {
                // Do not submit if autocomplete item has been selected.
                if(autocompleteSelected == false)
                    return SearchLinkFromKey(event,'/Product/AdvSearchResult.aspx?keywords=', this.value,'ctl00_uhGo','','/Product/SearchNotFound.aspx?keywords=');
            });
        });
    
         
    </script>
    <script type="text/javascript">
        var cookieDomain = '';
        setCookieSubValue('UlineCom', 'IsJavaScriptEnabled', 'true', 180, cookieDomain);

        var reloadCookieVal = getCookieSubValue('UlineCom', 'BackButtonReloadMe');
        var fullPagePath = document.getElementById("FullPagePath").value;

        reloadCookieVal = unescape(reloadCookieVal);
        fullPagePath = unescape(fullPagePath);

        if (reloadCookieVal != null
                    && (reloadCookieVal.toUpperCase() == fullPagePath.toUpperCase())
                    ) {

            // Add the "RemoveReloadCookie" Parameter if it does not exist.  It would exist in "Back Button" Scenarios
            if (fullPagePath.indexOf("RemoveReloadCookie=Y") < 0) {

                var nextParmDel = "?";
                if (fullPagePath.indexOf("?") >= 0) {
                    nextParmDel = "&";
                }
                fullPagePath = fullPagePath + nextParmDel + "RemoveReloadCookie=Y";
            }

setCookieSubValue('UlineCom', 'BackButtonReloadMe', null, null, cookieDomain);

			window.location.href = fullPagePath;
		}

	</script>

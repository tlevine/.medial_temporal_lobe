id=sl_svn3_37

><td class="source"><br></td></tr
><tr
id=sl_svn3_38

><td class="source">float threshold = 0.002;<br></td></tr
><tr
id=sl_svn3_39

><td class="source">int belowLimit = 1000;<br></td></tr
><tr
id=sl_svn3_40

><td class="source">int belowCounter = 0;<br></td></tr
><tr
id=sl_svn3_41

><td class="source">int state = 0; // 0=off 1=on<br></td></tr

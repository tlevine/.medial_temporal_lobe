id=sl_svn3_91

><td class="source">static void check_level(float level) {<br></td></tr
><tr
id=sl_svn3_92

><td class="source">	if (level &gt; threshold) {<br></td></tr
><tr
id=sl_svn3_93

><td class="source">		belowCounter = 0;<br></td></tr
><tr
id=sl_svn3_94

><td class="source">		if (state == 0) {<br></td></tr
><tr
id=sl_svn3_95

><td class="source">			// turn on<br></td></tr
><tr
id=sl_svn3_96

><td class="source">			trigger(true);<br></td></tr
><tr
id=sl_svn3_97

><td class="source">			state = 1;<br></td></tr
><tr
id=sl_svn3_98

><td class="source">		}<br></td></tr
><tr
id=sl_svn3_99

><td class="source">		return;<br></td></tr
><tr
id=sl_svn3_100

><td class="source">	}<br></td></tr
><tr
id=sl_svn3_101

><td class="source">	belowCounter++;<br></td></tr
><tr
id=sl_svn3_102

><td class="source">	if (state == 1 &amp;&amp; belowCounter &gt; belowLimit) {<br></td></tr
><tr
id=sl_svn3_103

><td class="source">		// turn off<br></td></tr
><tr
id=sl_svn3_104

><td class="source">		trigger(false);<br></td></tr
><tr
id=sl_svn3_105

><td class="source">		state = 0;<br></td></tr
><tr
id=sl_svn3_106

><td class="source">	}<br></td></tr
><tr
id=sl_svn3_107

><td class="source">}<br></td></tr
><tr
id=sl_svn3_108

><td class="source"><br></td></tr
><tr
id=sl_svn3_109

><td class="source">static void stream_read_callback(pa_stream *s, size_t l, void *) {<br></td></tr
><tr
id=sl_svn3_110

><td class="source">	const void *d;<br></td></tr
><tr
id=sl_svn3_111

><td class="source">	if (pa_stream_peek(s, &amp;d, &amp;l) &lt; 0) {<br></td></tr
><tr
id=sl_svn3_112

><td class="source">		show_error(&quot;pa_stream_peek() failed&quot;);<br></td></tr
><tr
id=sl_svn3_113

><td class="source">		return;<br></td></tr
><tr
id=sl_svn3_114

><td class="source">	}<br></td></tr
><tr
id=sl_svn3_115

><td class="source"><br></td></tr
><tr
id=sl_svn3_116

><td class="source">	const float *data = (const float*) d;<br></td></tr
><tr
id=sl_svn3_117

><td class="source">	unsigned samples = l / sizeof(float);<br></td></tr
><tr
id=sl_svn3_118

><td class="source">	float level = 0;<br></td></tr
><tr
id=sl_svn3_119

><td class="source"><br></td></tr
><tr
id=sl_svn3_120

><td class="source">	while (samples &gt;= channels) {<br></td></tr
><tr
id=sl_svn3_121

><td class="source">		for (unsigned c = 0; c &lt; channels; c++) {<br></td></tr
><tr
id=sl_svn3_122

><td class="source">			float v = fabs(data[c]);<br></td></tr
><tr
id=sl_svn3_123

><td class="source">			if (v &gt; level)<br></td></tr
><tr
id=sl_svn3_124

><td class="source">				level = v;<br></td></tr
><tr
id=sl_svn3_125

><td class="source">		}<br></td></tr
><tr
id=sl_svn3_126

><td class="source">		data += channels;<br></td></tr
><tr
id=sl_svn3_127

><td class="source">		samples -= channels;<br></td></tr
><tr
id=sl_svn3_128

><td class="source">	}<br></td></tr
><tr
id=sl_svn3_129

><td class="source">	pa_stream_drop(s);<br></td></tr
><tr
id=sl_svn3_130

><td class="source"><br></td></tr
><tr
id=sl_svn3_131

><td class="source">	//printf(&quot;Level : %f \n&quot;, level);<br></td></tr
><tr
id=sl_svn3_132

><td class="source">	check_level(level);<br></td></tr
><tr
id=sl_svn3_133

><td class="source">}<br></td></tr
><tr
id=sl_svn3_134

><td class="source"><br></td></tr
><tr
id=sl_svn3_135

><td class="source">static void stream_state_callback(pa_stream *s, void *) {<br></td></tr
><tr
id=sl_svn3_136

><td class="source">	const pa_channel_map *pcm;<br></td></tr

id=sl_svn3_48

><td class="source">void show_error(const char *txt, bool show_pa_error = true) {<br></td></tr
><tr
id=sl_svn3_49

><td class="source">	if (show_pa_error)<br></td></tr
><tr
id=sl_svn3_50

><td class="source">		fprintf(stderr, &quot;%s: %s \n&quot;, txt, pa_strerror(pa_context_errno(context)));<br></td></tr
><tr
id=sl_svn3_51

><td class="source">}<br></td></tr
><tr
id=sl_svn3_52

><td class="source"><br></td></tr
><tr
id=sl_svn3_53

><td class="source">void triggerApp(bool on) {<br></td></tr
><tr
id=sl_svn3_54

><td class="source">	if (on) {<br></td></tr
><tr
id=sl_svn3_55

><td class="source">		printf(&quot;on command %s \n&quot;, on_command);<br></td></tr
><tr
id=sl_svn3_56

><td class="source">		execl(on_command, on_command, NULL);<br></td></tr
><tr
id=sl_svn3_57

><td class="source">		return;<br></td></tr
><tr
id=sl_svn3_58

><td class="source">	}<br></td></tr
><tr
id=sl_svn3_59

><td class="source">	printf(&quot;off command %s \n&quot;, on_command);<br></td></tr
><tr
id=sl_svn3_60

><td class="source">	execl(off_command, off_command, NULL);<br></td></tr
><tr
id=sl_svn3_61

><td class="source">}<br></td></tr
><tr
id=sl_svn3_62

><td class="source"><br></td></tr
><tr
id=sl_svn3_63

><td class="source">void trigger(bool on) {<br></td></tr
><tr
id=sl_svn3_64

><td class="source">	int usbdev; /* handle to FTDI device */<br></td></tr
><tr
id=sl_svn3_65

><td class="source">	//char response[20]; /* receive buffer */<br></td></tr
><tr
id=sl_svn3_66

><td class="source">	printf(&quot;Trigger: %s \n&quot;, (on)?&quot;on&quot;:&quot;off&quot;);<br></td></tr
><tr
id=sl_svn3_67

><td class="source">	if (on_command &amp;&amp; off_command) {<br></td></tr
><tr
id=sl_svn3_68

><td class="source">		triggerApp(on);<br></td></tr
><tr
id=sl_svn3_69

><td class="source">		return;<br></td></tr
><tr
id=sl_svn3_70

><td class="source">	}<br></td></tr
><tr
id=sl_svn3_71

><td class="source">	//1)I added a system call to my code using stty for 115200 Baud 8N1:<br></td></tr
><tr
id=sl_svn3_72

><td class="source">	system(&quot;stty -F /dev/ttyUSB0 9600&quot;);<br></td></tr
><tr
id=sl_svn3_73

><td class="source">	//2) Later in my code I opened the device for reading/writing:<br></td></tr
><tr
id=sl_svn3_74

><td class="source">	usbdev = open(&quot;/dev/ttyUSB0&quot;, O_RDWR);<br></td></tr
><tr
id=sl_svn3_75

><td class="source">	if (usbdev == -1) {<br></td></tr
><tr
id=sl_svn3_76

><td class="source">		fprintf(stderr, &quot;Could not open USB device: /dev/ttyUSB0&quot;);<br></td></tr
><tr
id=sl_svn3_77

><td class="source">		return;<br></td></tr
><tr
id=sl_svn3_78

><td class="source">	}<br></td></tr
><tr
id=sl_svn3_79

><td class="source">	//3) I then used write to send, example uses 2 chars. These are defined prior to the call and can be any value from 0 to 255 for each char.:<br></td></tr
><tr
id=sl_svn3_80

><td class="source">	if (on)<br></td></tr
><tr
id=sl_svn3_81

><td class="source">		write(usbdev, on_control, 3);<br></td></tr
><tr
id=sl_svn3_82

><td class="source">	else<br></td></tr
><tr
id=sl_svn3_83

><td class="source">		write(usbdev, off_control, 3);<br></td></tr
><tr
id=sl_svn3_84


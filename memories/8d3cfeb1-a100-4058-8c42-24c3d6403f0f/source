id=sl_svn3_148

><td class="source">	case PA_STREAM_FAILED:<br></td></tr
><tr
id=sl_svn3_149

><td class="source">		show_error(&quot;Connection failed&quot;);<br></td></tr
><tr
id=sl_svn3_150

><td class="source">		break;<br></td></tr
><tr
id=sl_svn3_151

><td class="source"><br></td></tr

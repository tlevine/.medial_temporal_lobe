id=sl_svn3_234

><td class="source">int main(int argc, char *argv[]) {<br></td></tr
><tr
id=sl_svn3_235

><td class="source">	const char* usage = &quot;Checks the volume level of PulseAudio on both in and output. \n&quot;<br></td></tr
><tr
id=sl_svn3_236

><td class="source">		&quot;If the level gets above threshold on_script is executed. \n&quot;<br></td></tr
><tr
id=sl_svn3_237

><td class="source">		&quot;If the level is below threshold for some time off_script is executed. \n&quot;<br></td></tr
><tr
id=sl_svn3_238

><td class="source">		&quot;Threshold is between 0.000-1.000 default 0.002 \n&quot;<br></td></tr
><tr
id=sl_svn3_239

><td class="source">		&quot;Usage: volchecker on_script off_script threshold\n&quot;<br></td></tr
><tr
id=sl_svn3_240

><td class="source">		&quot;Example: volchecker /home/me/on.sh /home/me/off.sh 0.002\n&quot;;<br></td></tr
><tr
id=sl_svn3_241

><td class="source">	if (argc &gt; 1 &amp;&amp; argc &lt; 4) {<br></td></tr
><tr
id=sl_svn3_242

><td class="source">		printf(usage);<br></td></tr
><tr
id=sl_svn3_243

><td class="source">		return -1;<br></td></tr
><tr
id=sl_svn3_244

><td class="source">	} else if (argc == 4) {<br></td></tr
><tr
id=sl_svn3_245

><td class="source">		on_command = argv[1];<br></td></tr
><tr
id=sl_svn3_246

><td class="source">		off_command = argv[2];<br></td></tr
><tr
id=sl_svn3_247

><td class="source">		threshold = atof(argv[3]);<br></td></tr
><tr
id=sl_svn3_248

><td class="source">	}<br></td></tr
><tr
id=sl_svn3_249

><td class="source">	printf(usage);<br></td></tr
><tr
id=sl_svn3_250

><td class="source">	printf(&quot;Using default configuration. You probably want:\n&quot;);<br></td></tr
><tr
id=sl_svn3_251

><td class="source"><br></td></tr
><tr
id=sl_svn3_252

><td class="source">	trigger(false); // turn off to start with<br></td></tr
><tr
id=sl_svn3_253

><td class="source"><br></td></tr
><tr
id=sl_svn3_254

><td class="source">	int pa_ready = 0;<br></td></tr
><tr
id=sl_svn3_255

><td class="source"><br></td></tr
><tr
id=sl_svn3_256

><td class="source">	signal(SIGPIPE, SIG_IGN);<br></td></tr
><tr
id=sl_svn3_257

><td class="source"><br></td></tr
><tr
id=sl_svn3_258

><td class="source">	loop = pa_threaded_mainloop_new();<br></td></tr
><tr
id=sl_svn3_259

><td class="source"><br></td></tr
><tr
id=sl_svn3_260

><td class="source">	context = pa_context_new(pa_threaded_mainloop_get_api(loop), &quot;volchecker&quot;);<br></td></tr
><tr
id=sl_svn3_261

><td class="source"><br></td></tr
><tr
id=sl_svn3_262

><td class="source">	pa_context_connect(context, NULL, PA_CONTEXT_NOAUTOSPAWN, NULL);<br></td></tr
><tr
id=sl_svn3_263

><td class="source">	pa_context_set_state_callback(context, context_state_callback, &amp;pa_ready);<br></td></tr
><tr
id=sl_svn3_264

><td class="source"><br></td></tr
><tr
id=sl_svn3_265

><td class="source">	/* Run the main loop */<br></td></tr
><tr
id=sl_svn3_266

><td class="source">	if (pa_threaded_mainloop_start(loop) &lt; 0) {<br></td></tr
><tr
id=sl_svn3_267

><td class="source">		show_error(&quot;Main loop failed&quot;);<br></td></tr
><tr
id=sl_svn3_268

><td class="source">	}<br></td></tr
><tr
id=sl_svn3_269

><td class="source"><br></td></tr
><tr
id=sl_svn3_270

><td class="source">	while (true) {<br></td></tr
><tr
id=sl_svn3_271

><td class="source">		pa_threaded_mainloop_lock(loop);<br></td></tr
><tr
id=sl_svn3_272

><td class="source">		pa_threaded_mainloop_wait(loop);<br></td></tr
><tr
id=sl_svn3_273

><td class="source">		sleep(10);<br></td></tr
><tr
id=sl_svn3_274

><td class="source">	}<br></td></tr
><tr
id=sl_svn3_275

><td class="source"><br></td></tr
><tr
id=sl_svn3_276

><td class="source">	if (context)<br></td></tr
><tr
id=sl_svn3_277

><td class="source">		pa_context_unref(context);<br></td></tr
><tr
id=sl_svn3_278

><td class="source"><br></td></tr
><tr
id=sl_svn3_279

><td class="source">	pa_threaded_mainloop_free(loop);<br></td></tr
><tr
id=sl_svn3_280

><td class="source">	printf(&quot;Exiting&quot;);<br></td></tr
><tr
id=sl_svn3_281

><td class="source">	return 0;<br></td></tr
><tr
id=sl_svn3_282

><td class="source">}<br></td></tr
><tr
id=sl_svn3_283

><td class="source"><br></td></tr

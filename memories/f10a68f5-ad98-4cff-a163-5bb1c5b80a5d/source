        Scribd.Pingback.run(["/", "d", "o", "c", "u", "m", "e", "n", "t", "s", "/", "1", "5", "7", "2", "1", "1", "6", "3", "5", "/", "p", "i", "n", "g", "b", "a", "c", "k"].join(''));
      });
    

      Scribd.Backbone.document = new Scribd.Backbone.Document({'_id': 157211635});
      Scribd.Backbone.current_user = new Scribd.Backbone.WordUser({});
      Scribd.Backbone.document_comments_view = new Scribd.Backbone.DocumentCommentsView({el: $('#comments_container'), model: Scribd.Backbone.document});
    

      // Move the widget into the document viewer
      $('.outer_page_container').before($("#autogen_id_231947450"));
      Scribd.UI.Annotations.selector = selector = new Scribd.UI.Annotations.Selector("#autogen_id_231947450");
      Scribd.Backbone.annotations_collection_view = new Scribd.Backbone.AnnotationCollectionView({el: "#autogen_id_231947450", collection: Scribd.Backbone.document.get('annotations')});
      _.defer( function () {
        Scribd.Backbone.document.get('annotations').fetch({
          success: function() {
            _.defer( function () {
                Scribd.Backbone.document.get('comments').fetch();
            });
          }
        });
      });
    

      already_clicked = false
      $('div.ds_more_info').click(function(){
        $('.document_summary_info').toggle()
        if(!already_clicked){
          already_clicked = true;
          Scribd.track_event("experiment_content_discovery_v6", "Click","More Info", 1);
        }
      });
      trackEvent('control_content_discovery', 'Hit', 'Hit', 1, false);
      $('.document_summary_info').find('.flag_button').click(function(){
        Scribd.with_login("Flag Document", Scribd.current_doc.url, function(){
          Scribd.Lightbox.remote_open("lightbox_flag_document", "/read/flag_dialog", Scribd.toolbar_params());
        });
      });
      (new Scribd.UI.BuyDocManager("")).setup_buy_buttons(null);
    
new Scribd.SignIn("#autogen_id_231943876");
    Scribd.logged_in = false;

      Scribd.have_anonymous_current_user(null);
    

      Scribd.UI.library_menu = new Scribd.Library($("#autogen_id_231938737"), null);
    

      new Scribd.Header("#global_header");
    
new Scribd.LanguageSelector("#autogen_id_231939504");


<script type="text/javascript">
    (function($, EB) {
        if (!(jQuery.browser.msie && jQuery.browser.version === '6.0')) {
            $.getScript('http://ebmedia.eventbrite.com/s3-build/15098-rc2013-06-24-1d65b71/django/js/production/event_tracking.js', function() {
                
    EB.Facebook.ready(function() {
        var eventTrackingRouter = new EB.EventTracking.Router({
            FB: FB,
            ebServer: 'http://www.eventbrite.com',
            eventData: {"start_date_month_short": "Aug", "address_short": "1155 E 60th St, Chicago, IL", "name": "Do Good Data 2013", "start_date_day_of_the_week": "Fri", "url": "http://dogooddata2013-esv.eventbrite.com", "start_date_formatted": "Aug 23, 2013 7:30 AM CST", "repeats": false, "id": 6344752325, "categories": ["Conferences", "Classes"], "start_date": 1377268200000},
            saveOnInit: EB.EventTracking.userClickedSave,
            useLocalStorage: false,
            fetchCount: true,
            locale: 'en_US',
            sharedRouter: sharedRouter
        });
        EB.EventTracking.Analytics.init(
            eventTrackingRouter,
            {
                'eventId': 6344752325,
                'internalReferrer': '7'
            }
        );

        if (window.location.hash === '#view_list') {
            Backbone.history.navigate('', { trigger: true });
        } else {
            eventTrackingRouter.index();
        }

        if (!Backbone.History.started) {
            Backbone.history.start();
        }
    });

            });
        }
    })(window.jQuery, window.EB);
</script>


<script type="text/javascript">
    (function($, EB) {
        if (!(jQuery.browser.msie && jQuery.browser.version === '6.0')) {
            EB.namespace('EventTracking');
            EB.EventTracking.userClickedSave = false;
            $('#track_event').click(function(e) {
                e.preventDefault();
                EB.EventTracking.userClickedSave = true;
            });
        }
    })(window.jQuery, window.EB);
</script>




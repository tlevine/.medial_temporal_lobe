    <script type="text/javascript" src="/cms/lib5/NY01001205/Centricity/Template/2/c2-scripts/jquery.ba-resize.min.js"></script>
<script type="text/javascript" src="//extend.schoolwires.com/creative/scripts/joel/rotate.gallery/joel.photoGallery.rotate.v1.0.min.js"></script>
<script type="text/javascript">
   $(document).ready(function(){
       loadFlash();
       dynamicCSS();
       $(".sw-mystart-button.signin span").html("Teacher Sign In");
       //PHOTO GALLERY CONFIGURATION
		$("#sw-content-container10.region.ui-hp div.ui-widget.app.gallery").galleryRotate({
			"width"			: 720,
			"height" 		: 430,
			"useTitles"		: "no",
			"useDescriptions"	: "no",
			"useLinks"		: "yes",
			"static"		: "yes",
			"random" 		: "yes",
			"controls"		: "no",
			"itemize"		: "no",
			"itemizeThumbs"		: "no",
			"customClass"		: "no",
			"removeStyles"		: "no",
			"overlay"		: "no"
		});
		
		//SET THIS TO ON OR OFF FOR POINTS OF PRIDE SECTION
     		var setPoints = "on";
       
	       if(setPoints == "off") {
	       	$("div#hp-headlines-container, div#points-of-pride-photos").css("display" , "none");
	       } else {
	       	
	       }
   });
   function loadFlash(){
        if($("#search").size()){
            var so = new SWFObject("http://extend.schoolwires.com/creative/mystart/mystart-search.swf","mymovie", "220", "23", "8", "#FFFFFF");
            so.addParam("wmode", "transparent");
            so.addParam("allowScriptAccess", "always");
            so.addVariable("accountNum", accountNum);
            so.write("search");
        }
        
   }
   function dynamicCSS(){
   	$("#sw-mystart-right #sw-mystart-search").appendTo($("#sw-mystart-left"));
   	$("#sw-mystart-right").append("<div class='sw-mystart-button email'><a href='https://scarmail.scarsdaleschools.org/gw/webacc' target='_blank'><span>Email</span></a></div>");
        $("#gb-top-header").css("background-image", "url(/cms/lib5/NY01001205/Centricity/Template/2/scarsdale-top-header.jpg)");
	if("scarsdale" != "scarsdale"){
		$("div.sw-mystart-dropdown.schoollist").show();
		$("div#sw-content-container11").hide();
	}
   }
   
</script>


    //			FunctionName = '$.fn.wij';
    //			ScriptSRC = homeURL + 'http://cdn.wijmo.com/jquery.wijmo-complete.1.1.6.min.js';
    //			break;
            default:
		        FunctionName = 'Check' + ModuleName + 'Script';
                //ScriptSRC = ScriptSRC;
		        break;
        }

        if (eval("typeof " + FunctionName + " == 'function'")) {
            // do nothing, it's already included
        } else {
            // include file here
	        /*$.ajax({
		        type: "POST",
		        data: "{URL:'" + ScriptSRC + "'}",
		        url: homeURL + "/GlobalAssets/Proxies/CrossDomain.aspx/GetContent",
		        async: false,
		        dataType: "json",

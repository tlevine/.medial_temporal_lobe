<SCRIPT language=javascript>
$(document).ready(function(){
     accountNum = "010228447171107280258:aenm2px4azk";
     addSearch();
     addSynergy();
     addAssist();
});
function addSearch(){
    if($("form[@name='frmSites']").size()){
         $("form[@name='frmSites']").parent().addClass("schoolDropdown");
         $("form[@name='frmSites']").css("float", "left");
         $(".schoolDropdown").append("<div style='float:left' id='search'>search</div>");
    }
    else{
         $(".SWMyStartBar tr:eq(0)").prepend("<td><div id='search'>search</div></td>");
    }
   //embeds flash movie
}
function addSynergy(){
     synergyLink =$("#synergy a").attr("href");
     //alert(synergyLink);
     if(synergyLink != undefined){
          $(".SWMyStartBar tr:eq(1)").append("<td><div id='synergylink' style='float:left'><a href='"+synergyLink+"'><img src='http://extend.schoolwires.com/creative/mystart/Nav_Synergy.gif' border='0' /></a></div></td>");
     }
}
function addAssist(){
     assistLink =$("#assist a").attr("href");
     //alert(assistLink);
     if(assistLink != undefined){
          $(".SWMyStartBar tr:eq(1)").append("<td><div id='assistlink' style='float:left'><a href='"+assistLink+"'><img src='http://extend.schoolwires.com/creative/mystart/Nav_Assist.gif' border='0' /></a></div></td>");
     }
}</script>

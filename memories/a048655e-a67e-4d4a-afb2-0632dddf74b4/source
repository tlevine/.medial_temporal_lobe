			        var result = msg.d;
				        var script = document.createElement('script');
                        script.type = 'text/javascript';
                        script.text = result;
                        $('head').append(script);
		        }
	        });*/
	        
	        $.ajax({
		        url: ScriptSRC,
		        async: false,
		        cache: true,
		        success: function(msg){
			        var result = msg.d;
				        var script = document.createElement('script');
                        script.type = 'text/javascript';
                        script.text = result;
                        $('head').append(script);
		        }
	        });
        }
    }

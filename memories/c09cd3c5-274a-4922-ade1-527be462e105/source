	<script type="text/javascript">
    jQuery(document).ready(function(){
    var jq=jQuery;
    jq("a[rel^='prettyPhoto']").prettyPhoto({theme:"light_rounded",'social_tools':false}); //incase you want to use pretty photo for other images too, otherwise pass anything as selecter, we need this call to make PrettyPhoto available for later call
    /*get the gallery Id from the dom element eg. <div id='gallery_xyz_32'> gives 32'*/
    function pg_get_id(el){
        var eid=el.id;
        var beg=eid.lastIndexOf('_');//dom id is somename_something_idofgallery/media
        var id=eid.slice(beg+1);
        return id;
    }
    /* Bind click event*/
  
    jq(".bp-pretty-gallery a,.sitewide-galleries-sitewide a,.bp-gallery-embeded-galleries a").live("click",function(){

//find parent gallery id
    var gallery=jq(this).parent().get(0);
    var gallery_id=pg_get_id( gallery);
    if(!gallery_id||!jq(gallery).hasClass("gallery-type-photo"))
        return;//for those where we don't have the id
       fetch_gallery(gallery_id);

return false;
});


//for directory
jq("#galleries-dir-list #galleries-list .item-avatar a,#galleries-dir-list #galleries-list .item-title a").live("click",function(){
  //find the parent list-item
 
  
  var gallery=jq(this).parents("li.list-item").get(0);
   var gallery_id=pg_get_id( gallery);
      if(!gallery_id||!jq(gallery).hasClass("gallery-type-photo"))
        return;//for those where we don't have the id

      fetch_gallery(gallery_id);

return false;

});
//for user home page
jq(".bp-gallery .gallery-title a, .bp-gallery .gallery-cover a").live('click',function(){
 var gallery=jq(this).parents(".bp-gallery").get(0);
   var gallery_id=pg_get_id( gallery);
   if(!gallery_id||!jq(gallery).hasClass("gallery-type-photo"))
        return;//for those where we don't have the id
       fetch_gallery(gallery_id);

return false;
    
});

    jq("div.activity-inner .media-linked img").live("click",function (){
   var activity_li=jq(this).parents("li.new_media_update").get(0);

   var activity_li_id=jq(activity_li).attr("id");
   var eid=activity_li_id;
   var beg=eid.lastIndexOf('-');//dom id is somename_something_idofgallery/media
   var activity_id=eid.slice(beg+1);
        //return id;
   
  jq.post( ajaxurl, {
                action: 'fetch_gallery_media',
                'cookie': encodeURIComponent(document.cookie),
                'activity_id':activity_id
                },
        function(response){
                    var media=JSON.parse(response);//media is a object havinf 3 array elemnts media.titles, array of titles, media.description, array of descriptions, media.medias, array of media srcs
                    if(media.error!=undefined)
                        return true;//
                    jQuery.prettyPhoto.open(media.images,media.titles,media.description); //call Your Viwere here as we have arra of Image srcs, titles,desc

                    });
            return false;
});
function fetch_gallery(gallery_id){
 //fetch all the medias in the gallery using ajax
jq.post( ajaxurl, {
                action: 'fetch_gallery_media',
                'cookie': encodeURIComponent(document.cookie),
                'gallery_id':gallery_id
                },
        function(response){
                    var media=JSON.parse(response);//media is a object havinf 3 array elemnts media.titles, array of titles, media.description, array of descriptions, media.medias, array of media srcs
                    if(media.error!=undefined)
                        return true;//
                    jQuery.prettyPhoto.open(media.images,media.titles,media.description); //call Your Viwere here as we have arra of Image srcs, titles,desc
        
                    });   
}

});

</script>

$(document).ready(function(){

    if($('#stacky_num').length)
    {
	$("#cart_quantity").keyup(function(){
	    stacky_val = $("#cart_quantity").val();
	    if( isNaN(stacky_val) || (parseFloat(stacky_val) != parseInt(stacky_val) ) )
		stacky_val = 1;
	    $("#stacky_num").html(parseInt(stacky_val));
	    $("#stacky_price").html(stacky_val * parseFloat($('#price_diff').val()));
	});
    }

});



var reqs = new Array();
function pollFlickr(rand, cat)
{
	
		xmlhttp = GetXmlHttpObject();
		if (xmlhttp==null)
  		{			alert ("Your browser does not support AJAX!");  			return;  		}
		
		url = "flickrPickr.php";
		url = url + "?pic=" + rand;
		url = url + "&cat=" + cat;

		xmlhttp.onreadystatechange=stateChanged;
		xmlhttp.open("GET",url,true);
		xmlhttp.send(null);
		reqs.push(xmlhttp);
	//xmlDoc = xmlhttp.responseXML;
}	

function GetXmlHttpObject()
{
	if (window.XMLHttpRequest)
  	{
	  	// code for IE7+, Firefox, Chrome, Opera, Safari
  		return new XMLHttpRequest();
  	}
	if (window.ActiveXObject)
  	{
  		// code for IE6, IE5
  		return new ActiveXObject("Microsoft.XMLHTTP");
  	}
	return null;
}

function stateChanged()
{	
	
	if (xmlhttp.readyState==4)
  	{
  		for(i = 0; i< reqs.length; i++)
		{
			document.getElementById('flickrPhoto' + (i + 1)).innerHTML = reqs[i].responseText;
		}
  	}
}

</script>




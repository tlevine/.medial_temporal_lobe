<script type="text/javascript" >
  window.onload = function() {      
    var rows = document.getElementsByTagName("tr");    
    for(i = 0; i < rows.length; i++) {
    //manipulate rows <br>  
      if(i % 2 == 0){
         rows[i].className = "even"; 
      } else {
         rows[i].className = "odd";
      }
    }    
  }  
</script>

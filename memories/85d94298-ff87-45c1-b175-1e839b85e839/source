	<script type='text/javascript'>
		function HiDDen(value, nid, obj, cls){
			if(value == 1){
				document.getElementById('response-'+nid).style.display = 'none';
				document.getElementById('content-'+nid).style.display = 'block';
					if(obj.className == 'inactive')
			{
				obj.className = 'active';
			}
					document.getElementById('res').className = 'inactive';
				
			}else{
				document.getElementById('content-'+nid).style.display = 'none';
				document.getElementById('response-'+nid).style.display = 'block';
					if(obj.className == 'inactive')
					{
						obj.className = 'active';
					}
				document.getElementById('con').className = 'inactive';
			}
		
		
		}
	</script>

<script type="text/javascript" >

	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_700089").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/700089?view=json", function(json){
				jQuery("#upv_700089").empty();
				if (json.stats && json.stats.uniqueVisitors >= 0) {
					jQuery("#upv_700089").append(json.stats.uniqueVisitors);				
				} else {
					jQuery("#upv_700089").append("Not available");
				}
			});
		}
	}
</script>
		

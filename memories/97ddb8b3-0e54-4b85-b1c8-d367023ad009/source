                                                        <li class="post sidebar-post  top bf_dom sidebar-post-wide " rel:bf_bucket="track" track=""
                                    id="promoted-24-things-to-do-before-you-turn-30">
                                    	<div class="video-embed-sidebar video-embed-area" id="video_buzz_element_2489701" rel:thumb=""><span class="print">Video available at: <a href="http://youtube.com/watch?v=SsL-QzgF5Sk">http://youtube.com/watch?v=SsL-QzgF5Sk</a>.</span></div>
	<script type="text/javascript" charset="utf-8">
	/* <![CDATA[ */		
		BuzzLoader.register(function() {
			try {
				var agent=navigator.userAgent.toLowerCase();
				var is_mobile = agent.match(/iphone|ipod|android|ipad/);
							
				var vd = new bf2_terminal_Video;
				vd.detect_type("http://youtube.com/watch?v=SsL-QzgF5Sk");
				var src_link = "http://youtube.com/watch?v=SsL-QzgF5Sk";
				if (vd.type && vd.type != null) {
					src_link = vd.src_link;
				}

				var so = BF_initSwfObject(src_link,{width:336,height:224});

        // Flowplayer
				if (so.flowplayer) {
				  so.hide_controls = true;
				  so.height = 189;
					

					if (is_mobile) so.use_html5 = true;
					so.write("video_buzz_element_2489701");

				// SWF object / non-yt mobile
				} else if (is_mobile && vd.type == 'buzzfeed') {
					if (is_mobile) so.use_html5 = true;
					so.write("video_buzz_element_2489701");
				} else if ( (! is_mobile && so.installedVer && so.installedVer.major < 8) || ( is_mobile && vd.type != 'youtube') ) {
					$('video_buzz_element_2489701').addClassName('video-embed-noflash');
					$('video_buzz_element_2489701').style.height = '340px';
				} else {
					if (vd.type == 'youtube') {
						if(isIE8() || isIE7()) {
							Event.observe(window, 'beforeunload', function(){ //for ie8 we need to handle page exits in a special manner
								$$("object").each(function(vidPlayer) { //grab the player(s)
									vidPlayer.remove(); //remove it so js calls no longer occur on exit to non-existant objects
								});
							}, false);
							if(isIE8()) { //internet explorer doesn't have access to the src from SWFObject, so must pass in alternative manner
								so.addParam('wmode','opaque');
								$('video_buzz_element_2489701').fire("video:dfp", { src: src_link }); //fire off a message in a bottle
							}
                            so.addParam("wmode", "opaque");
							so.write($('video_buzz_element_2489701')); //IE8 can't handle <embed>, must use SWFObject to get <object>
						} else {
							if(window.autoplay != undefined) var autoplay = window.autoplay ? 1 : 0;
							var you_tube_markup = '';
							var video_pl = [{
								video_id: vd.id,
								title: '',
								author: '',
								uri: '',
								thumbnail: vd.thumbs[2]
							}];

							if (video_pl.length == 1) {
							
								var you_tube_tmpl = new Template('<embed id="embed_2489701" src="#{src}" type="application/x-shockwave-flash" allowscriptaccess="always" allowfullscreen="true" width="#{width}" height="#{height}" bgcolor="#000" wmode="opaque" class="ytvideo"></embed>');
								var you_tube_markup = you_tube_tmpl.evaluate({ src: src_link+'?version=3&hl=en&fs=1&enablejsapi=1&playerapiid=embed_2489701&autoplay='+ autoplay + '&rel=0&showinfo=0', width: 336, height: 224 });
							
							}

							$('video_buzz_element_2489701').innerHTML = you_tube_markup;
						}
	          } else if (vd.type == 'vimeo') {

	            var vimeo_tmpl = new Template('<iframe src="#{src}" width="#{width}" height="#{height}" frameborder="0"></iframe>');
	            var vimeo_markup = vimeo_tmpl.evaluate({ src: src_link+'?color=ee3322', width: 336, height: 224 });
	            $('video_buzz_element_2489701').innerHTML = vimeo_markup;
			} else {
					try{so.addParam("wmode", "opaque");} catch(e) {}
					so.write("video_buzz_element_2489701");
				}
					}
				} catch(e) {
					console.log(e);
				}

			}, 10);
	/* ]]> */
	</script>


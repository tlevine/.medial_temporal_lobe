$(document).ready(function(){

    if($('#stacky_num').length)
    {
	$("#cart_quantity").keyup(function(){
	    stacky_val = $("#cart_quantity").val();
	    if( isNaN(stacky_val) || (parseFloat(stacky_val) != parseInt(stacky_val) ) )

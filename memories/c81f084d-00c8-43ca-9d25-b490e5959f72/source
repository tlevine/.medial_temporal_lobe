<script type="text/javascript">
  
var isIE5=navigator.userAgent.toUpperCase().indexOf("MSIE 5")!=-1;
  
var targetElementID="spacerfooter", targetElementStyleOffset=160;

function adjustHeight() {
  if (document.getElementById) {
    var targetElement=document.getElementById(targetElementID),
        documentHeight, totalOffset;

    if (targetElement) {
      documentHeight=document.documentElement.offsetHeight;
      if (targetElement.offsetHeight<documentHeight-targetElement.offsetTop) {
        if (isIE5)
          totalOffset=targetElement.offsetTop;
          else totalOffset=targetElement.offsetTop+targetElementStyleOffset;
        targetElement.style.height=String(documentHeight-totalOffset)+'px';
      }
    }
  }
}

window.onresize=adjustHeight;
window.onload=adjustHeight;
   
  

  function toggleShow(choice,link,openClass,closeClass){
     $("#"+choice).toggle();
     $("#"+choice).addClass('subarticle');
     $(link).toggleClass(openClass);
     $(link).toggleClass(closeClass);
     /*
     if ($(link).hasClass(openClass)) {
        $(link).removeClass(openClass);
        $(link).addClass(closeClass);
     }else{
        $(link).addClass(openClass);
        $(link).removeClass(closeClass);
     }
     */
  }
    
  function createHelpie(event, something, content, helpiewidth){
    // Do something
  var $helpie = $("<div/>", {
      "id" : "helpietooltip",
      "class" : "helper",
      "html" : decodeURIComponent(content.replace(/\+/g,  " "))
    });
    var target = event.currentTarget || event.srcElement;
    console.log($helpie.height());
    $("body").append($helpie);
    $helpie.css({
      'position':'absolute',
      'width':helpiewidth+'px',
      'top': $(target).offset().top + 20,
      'left': $(target).offset().left - $(target).width() / 2
    });
  }
  
  function removeHelpie(){
    // Do something
    $("#helpietooltip").remove();
  }

  </script>






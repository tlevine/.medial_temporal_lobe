  <script type="text/javascript">
function hideMe(thisElement){
    $(thisElement).hide();
    }
    
function showOrHide(thisClass,divName,noun){
    
    
    
    //console.log('thisClass: ' + thisClass);
    if (noun == ''){
    showverb = '';
    hideverb = '';
    } else {
    showverb = 'View all ';
    hideverb = 'Hide all '; 
    }
if ($(thisClass).css('display') == "none"){
$(thisClass).fadeIn(100);
$('#spacerfooter').css('display','none');    
if (noun != ''){    
$(divName).html(hideverb + noun);   }
    
    }
else{
  $(thisClass).fadeOut(100);
  $('#spacerfooter').css('display','block');   
if (noun != ''){    
$(divName).html(showverb + noun); }
   
    }
}
  
  
</script>



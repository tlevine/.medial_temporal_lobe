//Preview mechanics
var tmpToggleHeight = new Object();
function toggleMe(theDiv){
	//return;
	//console.log($("#" + theDiv))
	if(!tmpToggleHeight[theDiv]){
		$("#" + theDiv)[0].style.height="8em"
		tmpToggleHeight[theDiv] = 6
	} else {
		$("#" + theDiv)[0].style.height="3.0em"
		tmpToggleHeight[theDiv] = 0	
	}
}

</script>

